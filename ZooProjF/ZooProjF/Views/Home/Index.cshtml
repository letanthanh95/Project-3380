@{
    ViewData["Title"] = "Home Page";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.title</title>
    <meta content="" name="descriptison">
    <meta content="" name="keywords">
    <link href="~/Content/img/icon.png" rel="icon">
    <link href="Content/font_icon/css/all.css" rel="stylesheet">


    <link href="~/Content/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Montserrat:300,400,500,700" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <link href="/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="/Content/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="/Content/vendor/ionicons/css/ionicons.min.css" rel="stylesheet">
    <link href="/Content/vendor/animate.css/animate.min.css" rel="stylesheet">
    <link href="/Content/vendor/venobox/venobox.css " rel="stylesheet">
    <link href="/Content/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">


    <!-- Template Main CSS File -->

    <link rel="stylesheet" href="/Content/style.css" type="text/css">
    <link rel="stylesheet" href="/Content/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="/Content/style_schedule.css" type="text/css">


</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header">
        <div class="container-fluid">

            <div id="logo" class="pull-left">
                <h1>

                    @Html.ActionLink("zoo", "Index", "Home", new { @style = "text-decoration:none;font-family: 'Dancing Script', cursive;" })

                </h1>

                <!-- Uncomment below if you prefer to use an image logo
                <a ><img src="Content/img/panda_logo.png" alt=""></a>-->
            </div>

            <nav id="nav-menu-container">
                <ul class="nav-menu">

                    <li class="menu-active">@Html.ActionLink("Home", "Index", "Home", new { @style = "text-decoration:none;" })</li>
                    <li>@Html.ActionLink("Tickets", "TicketIndex", "Home", new { @style = "text-decoration:none;" })</li>

                    <li>@Html.ActionLink("Shop", "ShopIndex", "Home", new { @style = "text-decoration:none;" })</li>
                    <li class="menu-has-children">
                        <a href="" style="text-decoration:none">Events</a>
                        <ul>
                            <li>@Html.ActionLink("Private Events", "Index", "Home", new { @style = "text-decoration:none;" })</li>
                            <li>@Html.ActionLink("Public Events", "Index", "Home", new { @style = "text-decoration:none;" })</li>

                        </ul>
                    </li>

                    <li>@Html.ActionLink("Login/Register", "Login", "Account", new { @style = "text-decoration:none;" })</li>
                </ul>
            </nav><!-- #nav-menu-container -->
        </div>
    </header><!-- End Header -->
    <!-- ======= Intro Section ======= -->
    <section id="intro">
        <div class="intro-container">
            <div id="introCarousel" class="carousel  slide carousel-fade" data-ride="carousel">

                <ol class="carousel-indicators"></ol>

                <div class="carousel-inner" role="listbox">

                    <div class="carousel-item active">
                        <div class="carousel-background"><img src="Content/img/intro-carousel/Panda.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>Creatures of China</h2> <!-----------------------text edits-------------->
                                <p>Come see our newest exhibit, on loan from the Beijing Zoo!</p>
                                <a class="btn-get-started scrollto" href="Index">Explore</a>
                            </div>
                        </div>
                    </div>

                    <div class="carousel-item">
                        <div class="carousel-background"><img src="Content/img/intro-carousel/Bird.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>Avians of the Americas</h2> <!-----------------------text edits-------------->
                                <p style="text-align:left">Our most popular exhibit yet, with the residents easily seen in our special arboreal setup!</p>
                                @Html.ActionLink("Explore", "Index", "Home", new { @class = "btn-get-started scrollto", @style = "text-decoration:none;" })
                            </div>
                        </div>
                    </div>

                    <div class="carousel-item">
                        <div class="carousel-background"><img src="Content/img/intro-carousel/Fish.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>Coral Curios</h2>
                                <p>Visit our carefully-maintained coral aquarium!</p>
                                @Html.ActionLink("Explore", "Index", "Home", new { @class = "btn-get-started scrollto", @style = "text-decoration:none;" })
                            </div>
                        </div>
                    </div>

                    <div class="carousel-item">
                        <div class="carousel-background"><img src="Content/img/intro-carousel/flock.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>The Pink Posse</h2>
                                <p>Become friends with the largest flamingo flock ever kept on display!</p>
                                @Html.ActionLink("Explore", "Index", "Home", new { @class = "btn-get-started scrollto", @style = "text-decoration:none;" })
                            </div>
                        </div>
                    </div>

                    <div class="carousel-item">
                        <div class="carousel-background"><img src="Content/img/intro-carousel/fox.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>Red Rioters</h2>
                                <p>Visit our rambunctious red pandas Rocket and Riot!</p>
                                @Html.ActionLink("Explore", "Index", "Home", new { @class = "btn-get-started scrollto", @style = "text-decoration:none;" })
                            </div>
                        </div>
                    </div>

                </div>

                <a class="carousel-control-prev" href="#introCarousel" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon ion-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>

                <a class="carousel-control-next" href="#introCarousel" role="button" data-slide="next">
                    <span class="carousel-control-next-icon ion-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>

            </div>
        </div>
    </section><!-- End Intro Section -->

    <main id="main">

        <!-- ======= Featured Services Section Section ======= -->
        <section id="featured-services">
            <div class="container">
                <div class="row">

                    <div class="col-lg-4 box">
                        <i class="fa fa-ticket"></i>
                        <h4 class="title">@Html.ActionLink("Ticket", "TicketIndex", "Home", new { @style = "text-decoration:none;" })></h4>
                        <p class="description">Group and company discounts available.</p>
                    </div>

                    <div class="col-lg-4 box box-bg">
                        <i class="fa fa-map-o"></i>
                        <h4 class="title">@Html.ActionLink("Maps", "Index", "Home", new { @style = "text-decoration:none;" })</h4>
                        <p class="description">Maps for every section of the zoo.</p>
                    </div>

                    <div class="col-lg-4 box">
                        <i class="fa fa-map-marker"></i>
                        <h4 class="title">@Html.ActionLink("Getting Here", "Index", "Home", new { @style = "text-decoration:none;" })</h4>
                        <p class="description">Directions to other popular nearby landmarks also included.</p>
                    </div>

                </div>
            </div>
        </section><!-- End Featured Services Section -->
        <!-- ======= About Us Section ======= -->
        <section id="about">
            <div class="container">

                <header class="section-header">
                    <h3>Acitivities</h3>
                    <p></p>                                         <!---simply removed this text-->
                </header>

                <div class="row about-cols">

                    <div class="col-md-4 wow fadeInUp">
                        <div class="about-col">
                            <div class="img">
                                <img src="Content/img/welcome.png" alt="" class="img-fluid">
                                <div class="icon"><i class="fas fa-hippo"></i></div>
                            </div>
                            <h2 class="title">@Html.ActionLink("Exhibits", "Index", "Home", new { @style = "text-decoration:none;" })</h2>
                            <p>
                                With over 40 filled exhibits, theres something for everyone here!
                            </p>
                        </div>
                    </div>

                    <div class="col-md-4 wow fadeInUp" data-wow-delay="0.1s">
                        <div class="about-col">
                            <div class="img">
                                <img src="Content/img/store.png" style="height:218px;width:350px" alt="" class="img-fluid">
                                <div class="icon"><i class="fab fa-shopify"></i></div>
                            </div>
                            <h2 class="title">@Html.ActionLink("Shop", "ShopIndex", "Shop", new { @style = "text-decoration:none;" })</h2>
                            <p>
                                Take a memory home in the form of commemorative photos or plushes.
                            </p>
                        </div>
                    </div>

                    <div class="col-md-4 wow fadeInUp" data-wow-delay="0.2s">
                        <div class="about-col">
                            <div class="img">
                                <img src="Content/img/events.jpg" style="height:218px;width:350px" alt=" " class=" img-fluid">
                                <div class="icon"><i class="fas fa-campground"></i></div>
                            </div>
                            <h2 class="title">@Html.ActionLink("Events", "Index", "Home", new { @style = "text-decoration:none;" })</h2>
                            <p>
                                Special tickets for special events!
                            </p>
                        </div>
                    </div>

                </div>

            </div>
        </section><!-- End About Us Section -->
